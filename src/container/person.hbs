<div>
    <main>
        <section>
            <section>
            {{#with person}}
            <div>
                <h1>{{name}}</h1>
            </div>

            <div>
                <b>
                    {{age}}
                    <br>
                    {{gender}}
                </b>
            </div>
        </section>

        <br>

        <section>
            <div>
                <address>
                    {{#with address}}
                    {{street}}
                    {{city}}
                    {{state}}
                    {{zip}}
                    {{country}}
                    {{/with}}
                </address>
            </div>
        </section>

        <br>
        <hr>

        <section>
            <div>
                {{#each education}}
                <div>
                    <h2>{{degree}}</h2>
                </div>
                <br>
                <em>
                    {{major}} {{graduationYear}}
                    <br>
                    {{university}}
                </em>
                {{/each}}
            </div>
        </section>

        <hr>

        <section>
            <div>
                {{#each workExperience}}
                <h3>{{company}}</h3>
                <strong>{{title}}</strong>
                <br>
                <em>
                    {{startDate}}
                    {{endDate}}
                    <br>
                    <br>
                    {{responsibilities}}
                    <br>
                </em>
            </div>
        </section>

        <section>
            <div>
                {{year_founded}}
                <br>
                {{industry}}
            </div>
        </section>

        <br>
        <hr>

        <section>
            <div>
                {{#each employees}}
                <b>{{name}}</b>

                <br>

                {{position}}

                <br>

                {{department}}
            </div>

            <br>
            <br>

            <div>
                {{#each projects}}

                <mark>Projects:</mark>

                <br>

                <b>{{name}}</b>

                <br>
                <br>

                <span> {{description}} </span>

                <br>
                <br>

                <em> {{status}} </em>
            </div>
            <br>
            <br>

            <div>
                {{#each teams}}
                <div>
                    <mark>Teams:</mark>
                    <br>
                    {{team_name}}
                </div>

                <br>

                <div>
                    {{#with team_leader}}
                    <b>{{name}}</b>
                    <br>
                    <em>{{title}}</em>
                    <br>
                    {{email}}
                    {{/with}}
                </div>

                <br>
                <br>

                <div>
                    {{#each team_members}}
                    <b>{{name}}</b>
                    <br>
                    <em>{{title}}</em>
                    <br>
                    {{email}}
                    <br>
                    {{skills}}
                    <br>
                    <br>
                </div>

                <br>

                <div>
                    {{#each projects}}
                    <b>{{name}}</b>

                    <br>
                    <br>

                    <span>{{description}}</span>

                    {{technologies}}
                </div>
                <br>

                <div>
                    {{#each team_members}}
                    <br>
                    <b>{{name}}</b>
                    <br>
                    <em>{{title}}</em>
                    <br>
                    {{/each}}
                </div>

                {{/each}}
                {{/each}}

                {{/each}}
            </div>
            {{/each}}
            {{/each}}
        </section>

            {{/each}}
          {{/with}}
        </section>
    </main>
</div>