<div>
   {{#with main}}
      <main>
        <section>
           {{>title big=true data="My education"}}
            <ul>
                {{#each educations}}
                <li>
                  {{name}}
                  {{#if isEnd}} {{/if}}
                  {{#unless isEnd}}
                  (Ще в процесі)
                  {{/unless}}
                </li>
                {{/each}}
            </ul>
        </section>

        <section>
          {{>title data="My certificates"}}
          <ul>
            {{#each certificates}}
              <li>
                {{name}}
                ({{id}})
              </li>
             {{/each}}
          </ul>
        </section>

      </main>
    {{/with}}
</div>
